    "babel-polyfill": "^6.5.0",
    "css-loader": "^0.22.0",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.1",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-scope": "^1.0.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "react-tap-event-plugin": "^0.2.1",
